{
  "name": "mscott",
  "description": "That we she said",
  "homepage_url": "https://github.com/mscott-extension/chrome",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "index.js"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "icons/icon16.jpeg",
    "32": "icons/icon32.jpeg",
    "48": "icons/icon48.jpeg",
    "128": "icons/icon128.jpeg"
  }
}